<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
	 jcr:primaryType="nt:unstructured">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="enigmatic/foundation/parsys"
		components="[group:Mobirise Blog]"
		acsComponentsLimit="{Long}14">
		<new jcr:primaryType="nt:unstructured"
			cq:emptyText="Drag blog post components here" />
		<slider jcr:primaryType="nt:unstructured"
			sling:resourceType="mobirise/components/blog/slider">
			<items jcr:primaryType="nt:unstructured"
				sling:resourceType="enigmatic/foundation/parsys"
				components="[mobirise/components/blog/slider/item]">
				<new jcr:primaryType="nt:unstructured"
					cq:emptyText="Drag slides here" />
				<item jcr:primaryType="nt:unstructured">
					<links jcr:primaryType="nt:unstructured"
						sling:resourceType="enigmatic/foundation/parsys"
						components="[mobirise/components/blog/slider/item/link]">
						<new jcr:primaryType="nt:unstructured"
							cq:emptyText="Drag links here" />
					</links>
				</item>
			</items>
		</slider>
	</content>
</jcr:root>