<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
	 jcr:primaryType="nt:unstructured">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="enigmatic/foundation/parsys">
		<new jcr:primaryType="nt:unstructured"
			cq:emptyText="Drag main content components or assets here" />
		<table jcr:primaryType="nt:unstructured">
			<header jcr:primaryType="nt:unstructured">
				<page jcr:primaryType="nt:unstructured">
					<author jcr:primaryType="nt:unstructured">
						<items jcr:primaryType="nt:unstructured"
							sling:resourceType="enigmatic/foundation/parsys"
							components="[enigmatic/test/table/header/text]">
							<new jcr:primaryType="nt:unstructured"
								cq:emptyText="Drag header texts here" />
						</items>
					</author>
				</page>
			</header>
			<rows jcr:primaryType="nt:unstructured"
				sling:resourceType="enigmatic/foundation/parsys"
				components="[enigmatic/test/table/row]">
				<new jcr:primaryType="nt:unstructured"
					cq:emptyText="Drag table rows here" />
				<row jcr:primaryType="nt:unstructured">
					<page jcr:primaryType="nt:unstructured">
						<author jcr:primaryType="nt:unstructured">
							<items jcr:primaryType="nt:unstructured"
								sling:resourceType="enigmatic/foundation/parsys"
								components="[enigmatic/test/table/row/text]">
								<new jcr:primaryType="nt:unstructured"
									cq:emptyText="Drag row texts here" />
							</items>
						</author>
					</page>
				</row>
			</rows>
		</table>
	</content>
</jcr:root>