<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
	jcr:primaryType="cq:Template"
	jcr:title="Mobirise Domain"
	jcr:description="Mobirise Domain Page Template to serve as the start of a domain tree"
	allowedParents="[/apps/mobirise/templates/brand]"
	allowedChildren="[/apps/mobirise/templates/lang]"
	ranking="{Long}1">
	<jcr:content jcr:primaryType="cq:PageContent"
		sling:resourceType="mobirise/pages/domain"
		cq:allowedTemplates="[/apps/mobirise/templates/.*]"
		cq:designPath="/etc/designs/mobirise">
		<configs jcr:primaryType="nt:unstructured"
			sling:resourceType="mobirise/global/configs" />
		<labels jcr:primaryType="nt:unstructured"
			sling:resourceType="mobirise/global/labels" />
		<content jcr:primaryType="nt:unstructured"
			sling:resourceType="enigmatic/foundation/parsys" />
	</jcr:content>
